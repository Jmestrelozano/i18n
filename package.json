{
  "name": "i18n-translates",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate-json": "node scripts/generatedJson.js",
    "test": "jest --watchAll --coverage",
    "test:units": "jest --passWithNoTests --silent --noStackTrace --runInBand units",
    "test:integrations": "jest --passWithNoTests --silent --noStackTrace --runInBand test/integrations"
  },
  "dependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-dom": "^6.5.0",
    "babel-jest": "^29.7.0",
    "i18next": "^23.7.18",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.7.0",
    "jest-dom": "^4.0.0",
    "react-testing-library": "^8.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
